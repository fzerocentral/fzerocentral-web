<h1>{{@model.filter.name}} - Delete an outgoing implication</h1>

<form id="delete-implication-form">
  <div class="select-field-wrapper">
    <FilterSelect
      @fieldId="target-filter-field"
      @label=""
      @controlInstance={{this.filterSelect}} />
  </div>

  <div class="error-message"></div>

  <div>
    <button type="button" {{action "deleteImplication"}}> Delete implication </button>
  </div>
</form>
